{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "rewrites": [{ "source": "/(.*)", "destination": "dist/server.js" }],
  "env": {
    "VERCEL_ENV": "production",
    "NODE_ENV": "production"
  },
  "functions": {
    "dist/server.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}

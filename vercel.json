{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "VERCEL_ENV": "production",
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run vercel-build"
}
